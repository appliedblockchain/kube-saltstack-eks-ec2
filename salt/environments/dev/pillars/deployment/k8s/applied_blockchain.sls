applied_blockchain:
    deployment:
        k8s:
            provider: aws
            apps: 
                - name: hello-world
                  replicas: 3
                  port: 8000
                  image: crccheck/hello-world
                  tag: latest # The use of latest is really bad. This field has to go.
                  domain: k8s-deploy-test.abtech.dev
                  cert64: {{ salt.nacl.dec('') }}
                  cert_key64: {{ salt.nacl.dec('')}}
                  public_access: true
                - name: emsurge-api
                  replicas: 1
                  port: 8080
                  image: appliedblockchain/emsurge-api
                  tag: latest # The use of latest is really bad. This field has to go.
                  domain: abtech.dev
                  cert64: {{ salt.nacl.dec('') }}
                  cert_key64: {{ salt.nacl.dec('')}}
                  public_access: true
                  storage:
                    size: 1Gi
                    mount_path: /datadrive/clusterstorage
